 []\n","\n","    for data in file_content:\n","        array_data = data.strip().split(',')\n","        if len(array_data) >= 6:\n","            array_data[3] = array_data[3].strip().split(' ')\n","            student_data.append(array_data)\n","\n","    return student_data\n","\n","# Function to set student data in the form\n","def set_student_data(data, student_index):\n","    first_name.value, last_name.value, mail.value = data[0], data[1], data[2]\n","    courses_dropdown.options = data[3]\n","    interesting_link.value, favorite_program.value = data[4], data[5]\n","\n","# Function to update student data in the file\n","def update_student_data(file_content, index, new_data):\n","    file_content[index] = new_data + '\\n'\n","    with open(file_path, 'w') as file:\n","        file.writelines(file_content)\n","\n","# Event handler for updating student data\n","def update_data_handler(b):\n","    global student_data, selected_student_index\n","    courses_str = \" \".join(courses_dropdown.options)\n","    new_string_data = f\"{first_name.value},{last_name.value},{mail.value},{courses_str},{interesting_link.value},{favorite_program.value}\"\n","\n","    update_student_data(student_data, selected_student_index, new_string_data)\n","    update_student_data_list()\n","\n","    with output:\n","        print(\"Student data updated successfully.\")\n","\n","# Function to update student data list\n","def update_student_data_list():\n","    global student_data, selected_student_index\n","    student_data[selected_student_index][0:6] = [first_name.value, last_name.value, mail.value, courses_dropdown.options,\n","                                                  interesting_link.value, favorite_program.value]\n","\n","# Selecting a student\n","def on_student_selection_handler(change):\n","    if change['type'] == 'change' and change['name'] == 'value':\n","        selected_index = student_dropdown.options.index(change['new'])\n","        if selected_index != -1:\n","            global student_data, selected_student_index\n","            selected_student_index = selected_index\n","            set_student_data(student_data[selected_index], selected_index)\n","\n","# Set up\n","update_button.on_click(update_data_handler)\n","student_dropdown.observe(on_student_selection_handler)\n","\n","# Initialize the form\n","def initialize_form():\n","    try:\n","        with open(file_path) as file:\n","            file_content = file.readlines()\n","            if file_content:\n","                global student_data, selected_student_index\n","                student_data = extract_student_data(file_content)\n","                student_options = [f\"{data[0]} {data[1]}\" for data in student_data]\n","                student_dropdown.options = student_options\n","                selected_student_index = 0\n","                set_student_data(student_data[0], 0)\n","            else:\n","                print(f\"File is empty: {file_path}\")\n","    except FileNotFoundError:\n","        print(f\"File not found at path: {file_path}\")\n","\n","initialize_form()\n","\n","# Display\n","display(student_dropdown, first_name, last_name, mail, courses_dropdown, interesting_link, favorite_program, update_button, output)"],"metadata":{"id":"OOVmki4jxX3o","colab":{"base_uri":"https://localhost:8080/","height":300,"referenced_widgets":["bc1ce2c67b5d4d6cb67367c38c14d5db","5bffbb6c75404d408ceb2b3230d20119","3f63f5b61542436dab06e12b6f041f77","f0fb02a931fd46219f75188b40edd1e6","a0b55e405b484c8b90bec453aa029a43","35670dfaf6904b5a901995be7e5d5b9e","77c00b38993a4038b48349803907f328","6739044a603b4b58aa9b1bd51f9cdc05","19b78af586b94ce392553fbfcb4be7b5","5d0fb6bd8da74b549e860f2da3aa8250","52e15671505949e5bda26423cbdca501","62f69f202597447190a70f707abbc8e3","7961e67ded2f4eab8148bb22509d0ae8","3ba815597bb74f738836f8d01382d67a","dc81e728de9b4fdf98c597feea50e633","0fb95b04174a4a6c9d400950bb300ffd","e6b4d52ef75e450b96f625a18a5b858c","273b9fae5c5b40e79a995d9b718f96d2","dfd2